{
  "name": "srcdev-design-system",
  "private": true,
  "type": "module",
  "scripts": {
    "cleanup": "rm -rf node_modules package-lock.json .nuxt dist",
    "build": "npm run build:i18n && nuxt build",
    "build:i18n": "node scripts/build-i18n.mjs",
    "build:i18n-json": "node scripts/build-i18n-json.mjs",
    "build:i18n:watch": "node scripts/build-i18n.mjs --watch",
    "build:i18n-json:watch": "node scripts/build-i18n-json.mjs --watch",
    "dev": "concurrently \"npm run build:i18n:watch\" \"nuxt dev\" --names \"i18n,nuxt\" --prefix-colors \"cyan,green\"",
    "dev:nuxt-only": "nuxt dev",
    "generate": "npm run build:i18n && nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@iconify-json/material-symbols": "1.2.34",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "2.0.0",
    "@nuxt/image": "1.11.0",
    "@nuxt/scripts": "0.11.13",
    "@nuxt/test-utils": "3.19.2",
    "@nuxtjs/i18n": "10.0.6",
    "@nuxtjs/storybook": "^9.0.1",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.5",
    "@unhead/vue": "2.0.14",
    "concurrently": "9.2.1",
    "eslint": "9.34.0",
    "eslint-plugin-storybook": "^9.1.3",
    "nuxt": "4.1.0",
    "nuxt-auth-utils": "0.5.24",
    "nuxt-security": "2.4.0",
    "srcdev-nuxt-components": "6.1.8",
    "srcdev-nuxt-forms": "6.1.13",
    "storybook": "^9.1.3",
    "vue": "3.5.21",
    "vue-router": "4.5.1"
  },
  "dependencies": {
    "@vueuse/core": "13.9.0",
    "zod": "4.1.5"
  },
  "overrides": {
    "@nuxtjs/storybook": {
      "storybook": "^9.1.3",
      "nuxt": "^4.0.0"
    },
    "@storybook-vue/nuxt": {
      "storybook": "^9.1.3",
      "nuxt": "^4.0.0"
    }
  }
}
