{
  "name": "srcdev-design-system",
  "private": true,
  "type": "module",
  "scripts": {
    "cleanup": "rm -rf node_modules package-lock.json .nuxt dist",
    "build": "npm run build:i18n && nuxt build",
    "build:i18n": "node scripts/build-i18n.mjs",
    "build:i18n-json": "node scripts/build-i18n-json.mjs",
    "build:i18n:watch": "node scripts/build-i18n.mjs --watch",
    "build:i18n-json:watch": "node scripts/build-i18n-json.mjs --watch",
    "dev": "concurrently \"npm run build:i18n:watch\" \"nuxt dev\" --names \"i18n,nuxt\" --prefix-colors \"cyan,green\"",
    "dev:nuxt-only": "nuxt dev",
    "generate": "npm run build:i18n && nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.1",
    "@iconify-json/gravity-ui": "1.2.10",
    "@iconify-json/material-symbols": "1.2.36",
    "@iconify-json/mdi": "1.2.3",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "2.0.0",
    "@nuxt/image": "1.11.0",
    "@nuxt/scripts": "0.11.13",
    "@nuxt/test-utils": "3.19.2",
    "@nuxtjs/i18n": "10.1.0",
    "@nuxtjs/storybook": "9.0.1",
    "@storybook-vue/nuxt": "9.0.1",
    "@storybook/addon-a11y": "9.1.6",
    "@storybook/addon-docs": "9.1.6",
    "@storybook/addon-vitest": "9.1.6",
    "@types/markdown-it": "14.1.2",
    "@unhead/vue": "2.0.14",
    "@vue/test-utils": "2.4.6",
    "concurrently": "9.2.1",
    "eslint": "9.35.0",
    "eslint-plugin-storybook": "9.1.6",
    "happy-dom": "18.0.1",
    "nuxt": "4.1.2",
    "nuxt-auth-utils": "0.5.25",
    "nuxt-security": "2.4.0",
    "playwright-core": "1.55.0",
    "srcdev-nuxt-components": "6.1.17",
    "srcdev-nuxt-forms": "6.1.17",
    "storybook": "9.1.6",
    "vitest": "3.2.4",
    "zod": "4.1.8"
  },
  "dependencies": {
    "@vueuse/core": "13.9.0",
    "markdown-it": "14.1.0"
  },
  "overrides": {
    "@nuxtjs/storybook": {
      "storybook": "9.1.6",
      "nuxt": "4.1.2"
    },
    "@storybook-vue/nuxt": {
      "storybook": "9.1.6",
      "nuxt": "4.1.2"
    }
  }
}
